#!/bin/sh

prefix=@prefix@
MYTEST=@MYTEST@
bindir=$prefix/bin
etcdir=@sysconfdir@

SUBNET=`/sbin/ifconfig eth0 | grep "inet addr" | cut -d. -f3`

mkdir -p $HOME/.tseries

case $SUBNET in
    20)                       region="FOU" ;;
    36|40)                    region="VV"  ;;
    48)                       region="VNN" ;;
    16|18|24|26|56|90|104|50) region="VA"  ;;
    *)                        region="VTK" ;;
esac

if [  -n "$MYTEST" ] ; then

$bindir/tseries.bin-TEST $@ -S $region -s $etcdir/tseriesTEST/tseries.ctl -T TEST

else

$bindir/tseries.bin $@ -S $region

fi




